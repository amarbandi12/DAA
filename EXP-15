#include <stdio.h>
#include <string.h>
int main() {
    char s[200];
    printf("Enter string: ");
    scanf("%s", s);
    int n = strlen(s);
    int start = 0;
    int printed = 0;

    printf("Output: [");
    for (int i = 1; i <= n; i++) {
        if (i == n || s[i] != s[start]) {
            int length = i - start;

            if (length >= 3) {
                if (printed) printf(",");
                printf("[%d,%d]", start, i - 1);
                printed = 1;
            }
            start = i; 
        }
    }
    printf("]\n");
    return 0;
}
