#include <stdio.h>
int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);
    int nums[n];
    printf("Enter %d elements:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }
    int max = nums[0];
    for(int i = 1; i < n; i++) {
        if(nums[i] > max) {
            max = nums[i];
        }
    }
    printf("Maximum element: %d\n", max);   
    return 0;
}
