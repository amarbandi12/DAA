#include <stdio.h>
int main() {
    int n;
    printf("Enter size of the list: ");
    scanf("%d", &n);
    if(n == 0) {
        printf("The list is empty.\n");
        return 0;
    }
    int arr[n], unique[n];
    int uniqueCount = 0;
    printf("Enter %d elements:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    for(int i = 0; i < n; i++) {
        int isDuplicate = 0;
        for(int j = 0; j < uniqueCount; j++) {
            if(arr[i] == unique[j]) {
                isDuplicate = 1;
                break;
            }
        }
        if(!isDuplicate) {
            unique[uniqueCount] = arr[i];
            uniqueCount++;
        }
    }
    printf("Unique elements: ");
    for(int i = 0; i < uniqueCount; i++) {
        printf("%d ", unique[i]);
    }
    printf("\n");
    return 0;
}
