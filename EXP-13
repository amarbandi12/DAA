#include <stdio.h>

int main() {
    int n;
    printf("Enter n: ");
    scanf("%d", &n);

    if (n == 0) {
        printf("Number of ways: 0\n");
        return 0;
    }
    if (n == 1) {
        printf("Number of ways: 1\n");
        return 0;
    }
    long a = 1, b = 1, c;
    for (int i = 2; i <= n; i++) {
        c = a + b;
        a = b;
        b = c;
    }
    printf("Number of ways: %ld\n", b);
    return 0;
}
